<h1>Crear Usuario</h1>
<%= form_with model: @user, local: true do |form| %>
  <%= form.label :username %>
  <%= form.text_field :username, required: true %>

  <%= form.label :email %>
  <%= form.email_field :email, required: true %>

  <%= form.label :password %>
  <%= form.password_field :password, required: true %>

  <%= form.submit "Registrar" %>
<% end %>
